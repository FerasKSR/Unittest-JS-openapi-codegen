{{#if parameters}}
{{#if @root.useOptions~}}
    {
    {{#each parameters}}
        {{{name}}}{{#if default}} = {{{default}}}{{/if}},
    {{/each}}
    }: {
    {{#each parameters}}
        {{{name}}},
    {{/each}}
    }
{{~else}}

    {{#each parameters}}
        {{{name}}}{{#if default}} = {{{default}}}{{/if}},
    {{/each}}
{{/if}}
{{/if}}
