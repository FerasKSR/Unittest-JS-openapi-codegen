{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Response for prolongation creation",
  "description": "Ответ на создание пролонгации",
  "type": "object",
  "properties": {
    "depositTermDate": {
      "type": "string",
      "format": "date",
      "description": "Дата списания средств"
    },
    "depositEndDate": {
      "type": "string",
      "format": "date",
      "description": "Дата окончания депозита"
    },
    "isEqualAccounts": {
      "type": "boolean",
      "description": "Признак совпадения счетов возврата и списания"
    },
    "replenishmentType": {
      "type": "string",
      "description": "Тип зачисления"
    },
    "paymentPeriodCode": {
      "type": "string",
      "description": "Периодичность выплаты"
    },
    "rate": {
      "type": "number",
      "description": "Ставка"
    },
    "depositGUID": {
      "type": "string",
      "description": "ID родительского депозита"
    },
    "term": {
      "type": "integer",
      "description": "Срок депозита"
    },
    "isReplenished": {
      "type": "boolean",
      "description": "Признак пополняемости"
    },
    "isRecallable": {
      "type": "boolean",
      "description": "Признак отзываемости"
    },
    "account": {
        "$ref": "../../common_response/accountDTO.json"
    }
  },
  "additionalProperties": false
}
